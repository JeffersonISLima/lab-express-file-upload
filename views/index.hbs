<h1>Formul√°rio envio de arquivos</h1>
<p>Envio de arquivos</p>

<section>
  <h2>Upload your photo</h2>
  <form action="/upload" method="post" enctype="multipart/form-data">
    <input type="text" name="pictureName">
    <input type="file" name="photo">
    <input type="submit" value="Save">
  </form>
</section>


<section>
  {{#each pictures}}
  <h3>{{this.name }}</h3>
  <img src="{{ this.path }}" alt="{{ this.originalName }}" style='width: 200px;' />
  {{/each}}
</section>